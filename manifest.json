{
  "name": "Justify",
  "description": "Makes multi-line text use the justified alignment.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icons/16.png"
  },
  "icons": {
    "128": "icons/128 2.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ]
}